{
    "name": "jenswiese/phpteda",
    "description": "Test Data Generation based on Faker",
    "keywords": ["testdata", "test-data generation", "faker", "fake-data"],
    "homepage": "https://github.com/jenswiese/phpteda",
    "license": "MIT",
    "authors": [
        {
            "name": "Jens Wiese",
            "email": "jens@dev.lohering.de",
            "homepage": "http://www.howtrueisfalse.de"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/console": "v2.2.0",
        "composer/composer": "dev-master",
        "fzaninotto/faker": "1.1.*@dev"
    },
    "require-dev": {
        "mockery/mockery": "dev-master",
        "mikey179/vfsStream": "dev-master"
    },
    "bin": [
        "bin/phpteda.php"
    ],
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": {
            "Phpteda": "src/",
            "Phpteda\\Test\\": "tests/",
            "PhptedaExamples": "examples/"
        }
    },
    "minimum-stability": "dev"
}
